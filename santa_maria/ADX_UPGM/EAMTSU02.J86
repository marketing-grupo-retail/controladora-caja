   !Retenciones
   call MSMTRETE02

   !Cheques
   call MSMTCHEQ02

   !Remates
   Call GRREMATES(02)
   
   !Deliverys
   CALL GRDOMICILIOS(02)

   !Cupones tienda
	 CALL GRCUPONES(02)

   !Pago Deuna
   CALL PAGO.DEUNA(02)
   
   !Pago Rappi
   CALL PAGO.RAPPI(02)
   
   !Claves Autorizacion
   CALL GRCLVAUT(02)


END FUNCTION
