FUNCTION CLEFT$(DATA$,N%) EXTERNAL
  STRING CLEFT$,DATA$
  INTEGER*2 N%
END FUNCTION

FUNCTION CRIGHT$(DATA$,N%) EXTERNAL
  STRING CRIGHT$,DATA$
  INTEGER*2 N%
END FUNCTION

FUNCTION CMID$(DATA$,N1%,N2%) EXTERNAL
  STRING CMID$,DATA$
  INTEGER*2 N1%,N2%
END FUNCTION

